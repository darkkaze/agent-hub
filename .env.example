# Agent Hub - Environment Variables Template
# Copy this file to deployment_vars.env and fill in your actual values

# Project Configuration
PROJECT_DOMAIN=https://yourdomain.com
NAMESPACE=your-namespace
INGRESS_HOST=yourdomain.com
TLS_SECRET_NAME=your-tls-secret
LETSENCRYPT_EMAIL=your-email@example.com

# PostgreSQL Configuration
POSTGRES_USER=your-db-user
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=your-database-name

# n8n Configuration
N8N_HOST=n8n.yourdomain.com
N8N_DB_NAME=n8n_db
N8N_DB_USER=n8n_user
N8N_DB_PASSWORD=your-n8n-db-password
N8N_ENCRYPTION_KEY=your-32-plus-character-encryption-key-here
